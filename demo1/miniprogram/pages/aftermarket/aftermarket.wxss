/* miniprogram/pages/order/order.wxss */
.head{
    height: 6vh;
    color: black;
    font-size: 25rpx;
    border-bottom: 2rpx solid #e9e9e9;
}
.select_tile{
    height: 100%;
    color: #b6c754;
    border-bottom: 4rpx solid;
}
.order{
    background-color: #fcfcfc;
    border-radius: 10rpx;
    margin-bottom: 20rpx;
    font-size: 25rpx;
}
.goods_img{
    width: 120rpx;
    height: 120rpx;
    border-radius: 10rpx;
}
.case{
    background-color: white;
    border-radius: 10rpx;
    box-shadow: 5rpx 5rpx 10rpx #888888;
    font-size: 25rpx;
    color: #888888;
}